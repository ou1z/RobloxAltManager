{
    "name": "Roblox-Alt-Manager",
    "description": "Let's you switch between alt accounts with ease!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["*://*.roblox.com/"],
    "permissions": ["cookies","storage","tabs"],
    "action": {
        "default_title": "Alt Manager",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": [ "click.mp3" ],
            "matches": [ "*://*.roblox.com/*" ]
        }
    ]
}